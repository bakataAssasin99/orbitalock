<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="DWord.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
    </sectionGroup>
  </configSections>
  <appSettings>
    <add key="addMinuteTimeCheckout" value="0"/>
    <add key="isUseDupKey" value="true"/>
    <add key="isMapRoomName" value="false"/>
    <add key="isMapRoomNo" value="false"/> 
    <add key="SqlOldVersion" value="false"/>
    <add key="ClientSettingsProvider.ServiceUri" value=""/> 
    <add key="Port" value="1"/>
    <add key="SystemCode" value="B6BBC182"/>
    <add key="HotelCode" value="1F584C78"/>
	<add key="isDBACCESS" value="true"/>
	<add key="ModeSetupKey" value="true"/>
	<add key="roomCodeFile" value="RoomCode.json"/>
	  
	  <!-- Connecting Setting-->
	<add key="ReaderModel" value="3"/>
	<!--You can check by from BTLock v5.7, accect: Menu -> Setting -> Reader Setting
        0 - RW-21
        1 - RW-26B
        2 - RW-33
        3 - RW-41
        -->
	<add key="Portcom" value="0"/>
	<!--You can check by from BTLock v5.7, accect: Menu -> Setting -> Reader Setting
    0 - Com 1
    1 - Com 2
    2 - com 3
    -->
	<add key="PowerSitchModel" value="1"/>
	<!--
    0 - 8601-1M-57BA
    1 - 8601-6M-57BA
    -->
	<add key="UseLiftFloor" value="4"/>
	<add key="MaxAddr" value="97"/>
	<add key="BeginAddr" value="96"/>
	<add key="EndAddr" value="97"/>
	<add key="SectorNo" value="0"/>
	<add key="PowerSwitchSectorId" value="FFFFFFFFFFFF"/>
	<add key="PowerSwitchSectorNo" value="15"/>
	<add key="isActivedLift" value="false"/>
	<add key="isActivedSwitch" value="false"/>
	<add key="AddLiftMinute" value="30"/>
	<add key="AddSwitchMinute" value="50"/>
  </appSettings>
  <userSettings>
    <DWord.Properties.Settings>
      <setting name="InitSuccess" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="Database" serializeAs="String">
        <value/>
      </setting>
      <setting name="COMPort" serializeAs="String">
        <value/>
      </setting>
      <setting name="AdelSystem" serializeAs="String">
        <value/>
      </setting>
      <setting name="Software" serializeAs="String">
        <value/>
      </setting>
      <setting name="TMEncoder" serializeAs="String">
        <value/>
      </setting>
      <setting name="ConnectionString" serializeAs="String">
        <value/>
      </setting>
      <setting name="LockFolder" serializeAs="String">
        <value>C:\Program Files (x86)\GLock\</value>
      </setting>
      <setting name="SystemCode" serializeAs="String">
        <value/>
      </setting>
      <setting name="LockSystem" serializeAs="String">
        <value>DWORD</value>
      </setting>
      <setting name="HotelCode" serializeAs="String">
        <value/>
      </setting>
    </DWord.Properties.Settings>
  </userSettings>
  <startup>
    
  <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/></startup>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri=""/>
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400"/>
      </providers>
    </roleManager>
  </system.web>
</configuration>
